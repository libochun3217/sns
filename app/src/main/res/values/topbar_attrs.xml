<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TopBar">
        <attr name="left">
            <enum name="blank" value="0" />
            <enum name="back" value="1" />
            <enum name="cancel" value="2" />
        </attr>
        <attr name="mid">
            <enum name="blank" value="0" />
            <enum name="title" value="1" />
            <enum name="undoredo" value="2" />
        </attr>
        <attr name="right">
            <enum name="blank" value="0" />
            <enum name="other" value="1" />
            <enum name="ok" value="2" />
            <enum name="highLight" value="3" />
        </attr>
        <attr name="color_dark" format="boolean" />
        <attr name="leftText" format="string|reference" />
        <attr name="rightText" format="string|reference" />
        <attr name="titleText" format="string|reference" />
        <attr name="titleDrawable" format="reference" />
        <attr name="showSeparator" format="boolean" />
    </declare-styleable>
</resources>